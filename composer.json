{
    "name": "adesigns/calendar-bundle",
    "type": "symfony-bundle",
    "description": "This bundle allows you to integrate the jQuery FullCalendar plugin into your Symfony2 application.",
    "keywords": ["calendar", "fullcalendar", "jquery calendar"],
    "homepage": "http://github.com/adesigns/calendar-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Yudin",
            "email": "mikeyudin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/twig-bundle": "~6|~7",
        "symfony/framework-bundle": "~6|~7",
        "friendsofsymfony/jsrouting-bundle": "~3",
        "doctrine/collections": "~2|~3",
        "symfony/yaml": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "~10",
        "symfony/phpunit-bridge": "~7.0",
        "symfony/debug": "~2|~3",
        "symfony/browser-kit" : "~6|~7",
        "symfony/finder": "~6|~7"
    },
    "autoload": {
        "psr-4": {
            "ADesigns\\CalendarBundle\\": "",
            "ADesigns\\CalendarBundle\\Tests\\": "Tests\\"
        }
    }
}
